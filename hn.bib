@article{gray2019,
  title = {{{OpenMDAO}}: An Open-Source Framework for Multidisciplinary Design, Analysis, and Optimization},
  author = {Gray, J. S. and Hwang, J. T. and Martins, J. R. R. A. and Moore, K. T. and Naylor, B. A.},
  year = {2019},
