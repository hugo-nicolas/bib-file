  author = {Wu, G. X. and Eatock Taylor, R.},
  year = {1995},
  journal = {Ocean Engineering},
  volume = {22},
  number = {8},
  pages = {785--798},
  issn = {0029-8018},
  doi = {10.1016/0029-8018(95)00014-C},
  abstract = {The two-dimensional nonlinear time domain free surface flow problem is analysed using potential flow theory. The problem is solved by a time marching method. At each time step two numerical approaches are used. One is based on the boundary element method in the complex plane. The complex potential is assumed to vary linearly within each element and the solution is obtained by imposing the boundary conditions at the nodes of the elements. The other approach is based on the finite element formulation. Triangular elements and linear shape functions are used. The solution is obtained by the Galerkin method. Numerical results are obtained for the wave elevation generated by a vertical wave maker. Results are also provided for a circular cylinder oscillating below the free surface. For these cases the finite element method is found to provide substantially more efficient computations than the boundary element method using equivalent discretizations.},
  langid = {english},
  file = {/Users/hu4o-nicolas/Zotero/storage/PVB4XKWN/1995 - Wu_Eatock Taylor - Time stepping solutions of the two-dimensional nonlinear wave radiation problem.pdf}
}

@article{wu2016,
  title = {Numerical Study on Cavitating Flow Due to a Hydrofoil near a Free Surface},
  author = {Wu, P. and Chen, J.},
  year = {2016},
  journal = {Journal of Ocean Engineering and Science},
  volume = {1},
  number = {3},
  pages = {238--245},
  issn = {2468-0133},
  doi = {10.1016/j.joes.2016.02.002},
  abstract = {A numerical strategy is proposed for a viscous uniform flow past a 2-D partially cavitating hydrofoil placed at a finite depth from the free surface. The flow was modeled by the Reynolds-averaged Navier--Stokes (RANS) equations. A finite-volume method with the SIMPLE scheme and k-{$\varepsilon$} turbulence model were employed for computations. The ``full cavitation model,'' which included the effects of vaporization, noncondensible gases and compressibility, was incorporated in the computation of cavitating flow. The cavity shape and free surface were updated iteratively till a reasonable convergence was reached. As for the determination of the free surface, the VOF approach was adopted. The test cases show the accuracy and stability of our procedure to capture the cavitating flow near the free surface.},
  langid = {english},
  keywords = {2-D hydrofoil,Cavitation,Free surface,Two-phase flow,Viscous flow},
  file = {/Users/hu4o-nicolas/Zotero/storage/ELP9GKHF/2016 - Wu_Chen - Numerical study on cavitating flow due to a hydrofoil near a free surface.pdf}
}

@article{xie2007a,
